<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>

  <%= f.label :artist_name%>
  <%= f.text_field :artist_name%><br>

  <%= f.fields_for :notes do |n| %>
    <%= n.label :content %>
    <%= n.text_field :content %><br>
    <%= n.label :content %>
    <%= n.text_field :content %><br>
    <%= n.label :content %>
    <%= n.text_field :content %><br>
  <% end %>

  <%= f.submit %>
<% end %>


